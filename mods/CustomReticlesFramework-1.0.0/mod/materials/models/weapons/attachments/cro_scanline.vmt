"UnlitTwoTexture"
{
	"$nocull" "1"
	"$nodecal" "1"
//	"$additive" "1"
	"$model" "1"
	"$nofog" "1"
	"$allowoverbright" "1"
	"$translucent" "1"
	"$alpha"	".4"

	//"$basetexture" "models\weapons\attachments\ammo_counter_screen_bg_col"
	"$basetexture" "models\holo_screens\holo_map_grid"
	"$Texture2" "effects\dev_scanline_sparse"
	"$color" "[1 1 1]"
	"$color2" "[1.8 1.8 2.3]"



	"$t2offset" 0
	"$t2scale" 1
	"$t2rot" 0
	"$scroll" "[0 1]"

	"Proxies"
	{
		//Horizontal movement speed
		"UniformNoise"
		{
			"minVal" 1
			"maxVal" 1.1
			"resultVar" "$scroll[0]"
		}

		//Vertical movement speed
//		"UniformNoise"
//		{
//			"minVal" 0
//			"maxVal" 10
//			"resultVar" "$scroll[1]"
//		}

		TextureScroll
		{
			"Texturescrollvar"  "$texture2transform"
			"Texturescrollrate" ".65"
			"texturescrollangle" "90"
		}
		"TextureScroll"
		{
		"Texturescrollvar" "$Basetexturetransform"
		"Texturescrollrate" ".65"
		"texturescrollangle" "90"
}

		//Move the 2nd texture using horiz and vert speed
		"TextureTransform"
		{
			//"translateVar" "$scroll"
			"scaleVar" "$t2scale"
			"rotateVar" "$t2rot"
			"resultvar" "$texture2transform"
		}
		
		//chevron flicker
		"UniformNoise"
		{
			"minVal" 0.45
			"maxVal" 0.5
			"resultVar" "$alpha"
		}
	}
}